    "# Hamiltonian MC linear regression"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_func_lr(hps, x, y):\n",
    "    assert len(hps) == 11\n",
    "    n, m = x.shape\n",
    "    alph = np.exp(hps[0])\n",
    "    beta = np.exp(hps[1])\n",
    "    w = np.array(hps[2:])\n",
    "    #lgwp = -0.5 * (m * np.log(2 * np.pi / alph) + w@w.T * alph)\n",
    "    lgwp = 0.5 * (m * np.log(alph / (2 * np.pi)) - alph*sum(w**2))\n",
    "    y_hat = x @ w\n",
    "#     print(f'MSE: {sum((y_hat -  y)**2) / y.shape[0]}')\n",
    "#     print(f'var: {np.var(y)}')\n",
    "    #prob_y_given_w_sig = stats.multivariate_normal.logpdf(y, mean=y_hat, cov=s2)\n",
    "    prob_y_given_w_sig = 0.5 * (-n * np.log(2*np.pi/beta) - sum((y - y_hat)**2) * beta)\n",
    "    #print(lgwp)\n",
    "    #print(prob_y_given_w_sig)\n",
    "    return -(lgwp + prob_y_given_w_sig) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_grad_lr(hps, x, y):\n",
    "    n, m = x.shape\n",
    "    g = np.zeros(11)\n",
    "    alph = np.exp(hps[0])\n",
    "    beta = np.exp(hps[1])\n",
    "    w = hps[2:]\n",
    "#     print(f'alpha: {alph}')\n",
    "#     print(f's2: {s2}')\n",
    "#     print(f'w: {w}')\n",
    "    #m = len(hps[2:])\n",
    "\n",
    "    g[0] = (0.5*m - 0.5*alph*w@w.T)\n",
    "    g[1] = (0.5*n - beta* 0.5 * sum((y - x@w)**2))\n",
    "    g[2:] = -(-x.T@y + x.T@x@w)*beta - alph * w\n",
    "    return -g"
   ]
  },
