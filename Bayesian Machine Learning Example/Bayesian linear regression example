    "# HMC classifier"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Transform y\n",
    "thres = 23\n",
    "train_idx = Y_train > thres\n",
    "test_idx = Y_test > thres\n",
    "Y_train_C = np.zeros_like(Y_train)\n",
    "Y_test_C = np.zeros_like(Y_test)\n",
    "Y_train_C[train_idx] = 1\n",
    "Y_test_C[test_idx] = 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sigmoid(x):\n",
    "    return 1 / (1+np.exp(-x))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_func_class(hps, x, y):\n",
    "    assert len(hps) == 10\n",
    "    n, m = x.shape\n",
    "    alph = np.exp(hps[0])\n",
    "    w = np.array(hps[1:])\n",
    "    lgwp = 0.5 * (m * np.log(alph / (2 * np.pi)) - alph*sum(w**2))\n",
    "    y_hat = sigmoid(x @ w)\n",
    "    log_prob_C_given_w = y @ np.log(y_hat) + (1-y) @ np.log(1 - y_hat)\n",
    "\n",
    "    return -(lgwp + log_prob_C_given_w) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "# de_dyhat = y / y_hat + (y-1) / (1-y_hat)\n",
    "# dyhat_dw = x*y_hat*(1-y_hat)\n",
    "# de_dw = x *(y * (1-y_hat) + (y-1) * y_hat)\n",
    "# de_dw = x.T @ (y - y_hat)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "metadata": {},
   "outputs": [],
   "source": [
    "def e_grad_class(hps, x, y):\n",
    "    n, m = x.shape\n",
    "    g = np.zeros(10)\n",
    "    alph = np.exp(hps[0])\n",
    "    w = hps[1:]\n",
    "    y_hat = sigmoid(x @ w)\n",
    "       \n",
    "    g[0] = (0.5*m - 0.5*alph*w@w.T)\n",
    "    g[1:] = x.T @ (y - y_hat) - alph * w\n",
    "    return -g"
   ]
  },
