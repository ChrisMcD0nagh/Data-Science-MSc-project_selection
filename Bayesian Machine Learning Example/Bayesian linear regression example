    "plt.plot(np.log(most_prob_alph), np.log(most_prob_beta), c='r', marker='x')\n",
    "#plt.title('Log probability of alpha and s2 given training data')\n",
    "plt.xlabel('Log alpha')\n",
    "plt.ylabel('Log beta');\n",
    "# plt.colorbar();\n",
    "# plt.subplot(2, 1, 2)\n",
    "# plt.contourf(X, Y, prob_y, 30, cmap='Blues') \n",
    "# plt.title('Probability of alpha and s2 given training data')\n",
    "# plt.xlabel('Log alpha')\n",
    "# plt.ylabel('Log s2')\n",
    "# plt.colorbar();\n",
    "#plt.savefig('./Report/report_figures/type2_mle.png')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "def compute_posterior(X, y, alph, s2):\n",
    "\n",
    "    Mu = np.linalg.solve(\n",
    "        (X.T @ X +\n",
    "         (s2*alph) *\n",
    "         np.eye(\n",
    "             X.shape[1])),\n",
    "        X.T @ y)\n",
    "    \n",
    "    SIGMA = np.linalg.inv(\n",
    "        (X.T @ X +\n",
    "         (s2*alph) *\n",
    "         np.eye(\n",
    "             X.shape[1]))) * s2\n",
    "    return Mu, SIGMA"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Train RMSE: 3.0117\n",
      "Train test_MSE: 3.0926\n",
      "Train R2: 0.9103\n",
      "Test R2: 0.9058\n",
      "Test ourperformance relative to lin. reg baseline: 3.2511%\n"
     ]
    }
   ],
   "source": [
    "Mu, SIGMA = compute_posterior(X_train, Y_train, alph=most_prob_alph, s2=(1/most_prob_beta))\n",
    "y_train_pred = X_train @ Mu\n",
    "y_test_pred = X_test @ Mu\n",
    "\n",
    "T2_MLE_train_MSE = sum((y_train_pred - Y_train)**2) / N_train\n",
    "T2_MLE_test_MSE = sum((y_test_pred - Y_test)**2) / N_test\n",
    "T2_MLE_train_R2 = 1 - T2_MLE_train_MSE / np.var(Y_train)\n",
    "T2_MLE_test_R2 = 1 - T2_MLE_test_MSE / np.var(Y_test)\n",
    "print(f'Train RMSE: {T2_MLE_train_MSE**0.5:.04f}')\n",
    "print(f'Train test_MSE: {T2_MLE_test_MSE**0.5:.04f}')\n",
    "print(f'Train R2: {T2_MLE_train_R2:.04f}')\n",
    "print(f'Test R2: {T2_MLE_test_R2:.04f}')\n",
    "print(f'Test ourperformance relative to lin. reg baseline: {(T2_MLE_test_R2 - test_R2)*100 / test_R2:.04f}%')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Variational Inference"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "exp_alpha: 0.0119\n",
      "exp_WWt: 755.1871\n",
      "exp_beta: 0.1102 (s2: 9.0703)\n"
     ]
    }
   ],
   "source": [
    "# beta = 1 / s2\n",
    "exp_beta = 0.1\n",
    "exp_alpha = 0.1\n",
    "M_c = M + 1\n",
    "a0 = 1e-4\n",
    "b0 = 1e-4\n",
    "an = a0 + M_c / 2\n",
    "cn  = a0 + N_train / 2\n",
    "for i in range(10):\n",
    "    sig = np.linalg.inv((X_train.T @ X_train) * exp_beta + exp_alpha * np.eye(M_c))\n",
    "    mu = sig @ X_train.T @ Y_train * exp_beta\n",
    "    WWt_exp = mu.T @ mu + np.trace(sig)\n",
    "    bn = b0 + WWt_exp * 0.5\n",
    "    dn = b0 + sum((X_train @ mu - Y_train)**2) / 2\n",
    "    exp_alpha = an / bn\n",
    "    exp_beta = cn / dn\n",
    "    \n",
    "print(f'exp_alpha: {exp_alpha:.04f}')\n",
    "print(f'exp_WWt: {WWt_exp:.04f}')\n",
    "print(f'exp_beta: {exp_beta:.04f} (s2: {1 / exp_beta:.04f})')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Train RMSE: 3.0117\n",
      "Test RMSE: 3.0926\n",
      "Train R2: 0.9103\n",
      "Test R2: 0.9058\n",
      "Test ourperformance relative to lin. reg baseline: 3.2511%\n"
     ]
    }
   ],
   "source": [
    "y_train_pred = X_train @ mu\n",
    "y_test_pred = X_test @ mu\n",
    "\n",
    "T2_v_train_MSE = sum((y_train_pred - Y_train)**2) / N_train\n",
    "T2_v_test_MSE = sum((y_test_pred - Y_test)**2) / N_test\n",
    "T2_v_train_R2 = 1 - T2_v_train_MSE / np.var(Y_train)\n",
    "T2_v_test_R2 = 1 - T2_v_test_MSE / np.var(Y_test)\n",
    "\n",
    "print(f'Train RMSE: {T2_v_train_MSE**0.5:.04f}')\n",
    "print(f'Test RMSE: {T2_v_test_MSE**0.5:.04f}')\n",
    "print(f'Train R2: {T2_v_train_R2:.04f}')\n",
    "print(f'Test R2: {T2_v_test_R2:.04f}')\n",
    "print(f'Test ourperformance relative to lin. reg baseline: {(T2_MLE_test_R2 - test_R2)*100 / test_R2:.04f}%')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "from scipy.special import gamma\n",
    "# Joint variational distribution\n",
    "w_mu = mu\n",
    "w_sig = sig\n",
    "a_alph = an\n",
    "b_alph = bn\n",
    "a_beta = cn\n",
    "b_beta = dn\n",
    "def calc_var_joint_prob(x_w, x_a, x_b):\n",
    "    sgn, logdet = np.linalg.slogdet(w_sig)\n",
    "    lmvn = ((logdet + (x_w - w_mu).T@np.linalg.inv(w_sig)@(x_w - w_mu) + N_train*np.log(2*np.pi))*-0.5)\n",
    "    t1 = a_alph*np.log(b_alph) + a_beta*np.log(b_beta) - np.log(gamma(a_alph)) - np.log(gamma(b_alph))\n",
    "    t2 = (a_alph - 1) * np.log(x_a) - b_alph * x_a + (a_beta - 1) * np.log(x_b) - b_beta * x_b \n",
    "    return lmvn + t2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "data": {
