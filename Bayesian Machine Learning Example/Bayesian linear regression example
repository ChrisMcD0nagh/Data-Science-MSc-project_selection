    "y_ax = np.linspace(-5,0,100) # beta range\n",
    "\n",
    "# fix other coefficients in x_w (w2 will be altered)\n",
    "x_w = np.copy(w_mu)\n",
    "\n",
    "alph = exp_alpha\n",
    "\n",
    "X, Y = np.meshgrid(x_ax, y_ax)\n",
