    "# Bayesian Linear regression"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Type-2 maximum likelihood"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Marginal Likelihood\n",
    "def compute_log_marginal(X, y, alph, beta):\n",
    "    cov = np.eye(X.shape[0]) / beta + X@X.T / alph\n",
    "#     lgp = stats.multivariate_normal.logpdf(y.T, cov=cov)\n",
    "    sgn, logdet = np.linalg.slogdet(cov)\n",
    "    lgp = ((logdet + y.T@np.linalg.inv(cov)@y + y.shape[0]*np.log(2*np.pi))*-0.5)\n",
    "    return lgp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "application/vnd.jupyter.widget-view+json": {
       "model_id": "b127d73f6afd4d67804e4757189f9fa7",
       "version_major": 2,
       "version_minor": 0
      },
      "text/plain": [
       "HBox(children=(HTML(value=''), FloatProgress(value=0.0), HTML(value='')))"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n"
     ]
    }
   ],
   "source": [
    "x_ax = np.linspace(-10,0, 100) # alpha range\n",
    "y_ax = np.linspace(-5,0,100) # beta range\n",
    "X, Y = np.meshgrid(x_ax, y_ax)\n",
    "log_prob_y = np.zeros_like(X)\n",
    "for row in tqdm(range(X.shape[0])):\n",
    "    for col in range(X.shape[1]):\n",
    "        alph = np.exp(X[row, col])\n",
    "        beta = np.exp(Y[row, col])\n",
    "        log_prob_y[row,col] = compute_log_marginal(X_train, Y_train, alph, beta)\n",
    "        #print(log_prob_y[row,col])\n",
    "\n",
    "prob_y = np.exp(log_prob_y) "
   ]
  },
