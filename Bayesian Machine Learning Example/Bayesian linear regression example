     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Stanardise inputs\n",
    "train_data_stdize = train_data.apply(lambda x: (x-x.mean())/x.std() if x.name != 'Heating Load' else x)\n",
    "test_data_stdize = test_data.apply(lambda x: (x-x.mean())/x.std() if x.name != 'Heating Load' else x)\n",
    "\n",
    "# Add constant\n",
    "train_data_stdize.insert(0, 'const', 1)\n",
    "test_data_stdize.insert(0, 'const', 1)\n",
    "train_data_stdize"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Train RMSE: 3.0116\n",
      "Train test_MSE: 3.5299\n",
      "Train R2: 0.9103\n",
      "Test R2: 0.8773\n"
     ]
    }
   ],
   "source": [
    "# Baseline linear regression\n",
    "X_train = np.array(train_data_stdize.loc[:,'const':'Glazing Area Distribution'],dtype='float')\n",
    "X_test = np.array(test_data_stdize.loc[:,'const':'Glazing Area Distribution'],dtype='float')\n",
    "Y_train = np.array(train_data_stdize['Heating Load'],dtype='float')\n",
    "Y_test = np.array(test_data_stdize['Heating Load'],dtype='float')\n",
    "\n",
    "coefs = np.linalg.solve(\n",
    "        (X_train.T @ X_train),\n",
    "        X_train.T @ Y_train)\n",
    "train_fit = np.dot(X_train, coefs)\n",
    "test_fit = np.dot(X_test, coefs)\n",
    "train_MSE = sum((train_fit - Y_train)**2) / N_train\n",
    "test_MSE = sum((test_fit - Y_test)**2) / N_test\n",
    "train_R2 = 1 - train_MSE / np.var(Y_train)\n",
    "test_R2 = 1 - test_MSE / np.var(Y_test)\n",
    "print(f'Train RMSE: {train_MSE**0.5:.04f}')\n",
    "print(f'Train test_MSE: {test_MSE**0.5:.04f}')\n",
    "print(f'Train R2: {train_R2:.04f}')\n",
    "print(f'Test R2: {test_R2:.04f}')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
